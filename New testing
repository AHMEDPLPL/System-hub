local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AHMEDPLPL/Library-/main/New%20happy%20fucking%20test')))()

local Window = OrionLib:MakeWindow({Name = "System hub V3",IntroText = "loading...",IntroIcon = "rbxassetid://7733666258" })

local Tab = Window:MakeTab({
Name = "Main",
Icon = "rbxassetid://7743875962",
PremiumOnly = false
})

