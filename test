Tab:AddButton({
  Name = "Anti-Cheat",
  Callback = function()
        local mt = getrawmetatable(game)
local ncallsa = mt.__namecall
  setreadonly(mt, false)
  mt.__namecall = newcclosure(function(...)
    local args = {...}
    if not checkcaller() and getnamecallmethod() == "Cheat" then
      return nil
    end
    return ncallsa(...)
  end)
  setreadonly(mt, true)

print("Anti-Cheat Loaded")
    end    
})

